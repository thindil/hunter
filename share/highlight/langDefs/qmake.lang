
Description="QMake Project"

Categories = {"config", "script"}

Keywords={
  { Id=1,
    List={"basename", "contains", "count", "", "dirname", "error", "eval", 
        "exists", "find", "for", "include", "infile", "", "isEmpty", "join", "member", 
